{
  "name": "keploy",
  "displayName": "Keploy",
  "description": "Streamline testing with the power of Keploy, directly in your favorite IDE.",
  "version": "1.0.0",
  "publisher": "Keploy",
  "icon": "media/logo.png",
  "pricing": "Free",
  "repository": {
    "type": "git",
    "url": "https://github.com/keploy/vscode-extension"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/keploy"
  },
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "Keploy-Sidebar",
          "title": "Keploy",
          "icon": "media/logo.png"
        }
      ]
    },
    "views": {
      "Keploy-Sidebar": [
        {
          "id": "Keploy-Sidebar",
          "name": "Keploy",
          "icon": "media/logo.png",
          "type": "webview",
          "webviewOptions": {
            "retainContextWhenHidden": true
          }
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "keploy.updateKeploy"
        },
        {
          "command": "keploy.getLatestVersion"
        },
        {
          "command": "keploy.KeployVersion"
        },
        {
          "command": "keploy.viewChangeLog"
        },
        {
          "command": "keploy.viewDocumentation"
        },
        {
          "command": "keploy.SignIn",
          "group": "navigation",
          "when": "keploy.signedIn != true"
        }
      ],
      "explorer/context": [
        {
          "command": "keploy.doSomething",
          "when": "resourceLangId == markdown",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "keploy.SignIn",
        "title": "Sign In here",
        "when": "keploy.signedIn != true"
      },
      {
        "command": "keploy.updateKeploy",
        "title": "Update your Keploy"
      },
      {
        "command": "keploy.KeployVersion",
        "title": "View Your Keploy Version"
      },
      {
        "command": "keploy.getLatestVersion",
        "title": "Get Latest Version"
      },
      {
        "command": "keploy.viewChangeLog",
        "title": "View Change Log"
      },
      {
        "command": "keploy.viewDocumentation",
        "title": "View Docs"
      },
      {
        "command": "keploy.doSomething",
        "title": "Do Something"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "rollup": "rollup -c -w --bundleConfigAsCjs",
    "watch": "tsc -watch -p ./",
    "debug": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@smui/button": "^7.0.0",
    "@smui/card": "^7.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.42",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "rollup": "^4.12.0",
    "rollup-plugin-svelte": "^6.0.0",
    "svelte-check": "^3.6.6",
    "svelte-preprocess": "^5.1.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@material-design-icons/svg": "^0.14.13",
    "@types/express": "^4.17.21",
    "@types/open": "^6.2.1",
    "express": "^4.19.2",
    "google-auth-library": "^9.12.0",
    "js-yaml": "^4.1.0",
    "lottie-web": "^5.12.2",
    "open": "^10.1.0",
    "sinon": "^17.0.1",
    "svelte": "^4.2.12",
    "uuid": "^10.0.0",
    "yaml": "^2.4.2"
  }
}
